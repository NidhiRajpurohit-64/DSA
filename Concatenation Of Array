def getConcatenation(nums):
    # Concatenate nums with itself
    return nums + nums

# Example 1
nums1 = [1, 2, 1]
ans1 = getConcatenation(nums1)
print("Input:", nums1)
print("Output:", ans1)

# Example 2
nums2 = [1, 3, 2, 1]
ans2 = getConcatenation(nums2)
print("Input:", nums2)
print("Output:", ans2)
